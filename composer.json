{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The skeleton application for the Laravel framework.",
  "keywords": [
    "laravel",
    "framework"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "brian2694/laravel-toastr": "^5.59",
    "fakerphp/faker": "^1.24",
    "guzzlehttp/guzzle": "^7.2",
    "intervention/image": "^3.11",
    "laravel/framework": "^10.10",
    "laravel/sanctum": "^3.2",
    "laravel/socialite": "^5.14",
    "laravel/tinker": "^2.9",
    "laravel/ui": "^4.2",
    "mews/purifier": "^3.4",
    "nexmo/client": "^2.4",
    "nunomaduro/termwind": "^1.15",
    "nwidart/laravel-modules": "*",
    "propaganistas/laravel-phone": "^5.1",
    "pusher/pusher-php-server": "^7.2",
    "spatie/laravel-permission": "^6.7",
    "spatie/laravel-sitemap": "^7.0",
    "stevebauman/location": "^7.3.2",
    "stripe/stripe-php": "^13.18",
    "twilio/sdk": "^7.15",
    "xgenious/installer": "^1.6.0",
    "xgenious/paymentgateway": "dev-main",
    "xgenious/xgapiclient": "^3.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.14",
    "laravel/pint": "^1.13",
    "laravel/sail": "^1.26",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^7.0",
    "phpunit/phpunit": "^10.5",
    "spatie/laravel-ignition": "^2.4",
    "fakerphp/faker": "^1.9.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/",
      "Modules\\": "Modules/"

    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": [
      "app/Helpers/Helpers.php",
      "app/Helpers/module-helper.php"
    ],
     "exclude-from-classmap": [
        "Modules/*/database/migrations/",
        "database/migrations/"
    ]
  },

  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
      "@php artisan migrate --graceful --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "ocramius/package-versions": true,
      "pestphp/pest-plugin": true,
      "php-http/discovery": true,
      "wikimedia/composer-merge-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
        {
            "type": "path",
            "url": "packages/paymentgateway"
        }
  ]
}
